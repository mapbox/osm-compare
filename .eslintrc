{
  "extends": "mourner",
  "rules": {
    "space-before-function-paren": 0,
    "indent": [2, 2],
    "global-require": 1,
    "consistent-return": 0,
    "no-unused-vars": 0,
    "camelcase": 0,
    "valid-jsdoc": 2,
    "comma-dangle": ["error", "never"],
    "operator-linebreak": [0],
    "object-curly-spacing": "warn"
  }
}
