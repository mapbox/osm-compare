{
  "compareFunction": "invalid_tag_modification",
  "fixtures": [
    {
      "description": "Test modification to primary tag of a feature",
      "expectedResult": {
        "result:invalid_tag_modification": true
      },
      "newVersion": {
        "type": "Feature",
        "properties": {
          "highway": "yes"
        },
        "geometry": null
      },
      "oldVersion": {
        "type": "Feature",
        "properties": {
          "railway": "yes"
        },
        "geometry": null
      }
    },
    {
      "description": "Test modification to primary tag's value of a feature",
      "expectedResult": false,
      "newVersion": {
        "type": "Feature",
        "properties": {
          "highway": "yes"
        },
        "geometry": null
      },
      "oldVersion": {
        "type": "Feature",
        "properties": {
          "highway": "no"
        },
        "geometry": null
      }
    },
    {
      "description": "Test feature with no primary tag",
      "expectedResult": false,
      "newVersion": {
        "type": "Feature",
        "properties": {
          "name": "Bengaluru"
        },
        "geometry": null
      },
      "oldVersion": {
        "type": "Feature",
        "properties": {
          "name": "Bengaluru"
        },
        "geometry": null
      }
    },
    {
      "description": "Test deletion of primary tag of a feature",
      "expectedResult": {"result:invalid_tag_modification":true},
      "newVersion": {
        "type": "Feature",
        "properties": {
          "name": "Bengaluru"
        },
        "geometry": null
      },
      "oldVersion": {
        "type": "Feature",
        "properties": {
          "name": "Bengaluru",
          "highway": "yes"
        },
        "geometry": null
      }
    },
    {
      "description": "Test addition of primary tag of a feature",
      "expectedResult": false,
      "newVersion": {
        "type": "Feature",
        "properties": {
          "name": "Bengaluru",
          "highway": "yes"
        },
        "geometry": null
      },
      "oldVersion": {
        "type": "Feature",
        "properties": {
          "name": "Bengaluru"
        },
        "geometry": null
      }
    },
    {
      "description": "Test addition of a second primary tag to a feature",
      "expectedResult": false,
      "newVersion": {
        "type": "Feature",
        "properties": {
          "sport": "soccer",
          "leisure": "sports_centre"
        },
        "geometry": null
      },
      "oldVersion": {
        "type": "Feature",
        "properties": {
          "leisure": "sports_centre"
        },
        "geometry": null
      }
    },
    {
      "description": "Test deletion of a second primary tag to a feature. The feature just became more specific",
      "expectedResult": {},
      "newVersion": {
        "type": "Feature",
        "properties": {
          "leisure": "sports_centre"
        },
        "geometry": null
      },
      "oldVersion": {
        "type": "Feature",
        "properties": {
          "sport": "soccer",
          "leisure": "sports_centre"
        },
        "geometry": null
      }
    },
    {
      "description": "When all properties of a feature are removed, assuming that the feature is being moved to a relation",
      "expectedResult": {},
      "newVersion": {
        "type": "Feature",
        "properties": {},
        "geometry": null
      },
      "oldVersion": {
        "type": "Feature",
        "properties": {
          "railway": "yes"
        },
        "geometry": null
      }
    }
  ]
}
